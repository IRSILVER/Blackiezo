local key = _G.Key
local check = "https://keychecking.000webhostapp.com/check.php?key=" .. key
if game:HttpGet(check) == "Whitelisted" then
loadstring(game:HttpGet("https://pastebin.com/raw/zzGDeWjC"))()
else
function notify(msg)     game.StarterGui:SetCore("SendNotification", {
Title = "Mining simulator 2";         
Text = msg;         
Duration = 6;     
})
end
notify("Incorrect key, to get the key join the discord, link copied to clipboard")
setclipboard("https://discord.gg/rdDnnMYkuy")
end
